//GCD BY RESURSION 
/*
#include<iostream>
#include<string>
using namespace std;
int gcd(int a , int b){
    // if(b==0){
    //     return a;
    // }
    // else{
    //     return gcd(b,a%b);
    // }
    if(b==0){
        return a;
    }
    else{
        return gcd(b,a-b);
    }
}
int main(){
int a,b;
cin>>a>>b;
int ans = gcd(a,b);
cout<<ans;
cout<<"LCM(a,b)="<<a*b/ans;

}
*/

Tower Of Hanoii */
void TOH(int n , string scr, string hel, string des){
  int count=0;
  if(n==1){
    cout<<"Transfer the disc1 from "<<scr<<" to "<<des<<endl;
    return;
  }
 TOH(n-1,scr,des,hel);
 cout<<"Transfer the disc"<<n<<" from "<<scr<<" to "<<des<<endl;
 TOH(n-1,hel,scr,des);
}
int main(){
  int n;
  cout<<" Enter the number of disc:"<<endl;
  cin>>n;
  TOH(n,"S","H","D");
}
