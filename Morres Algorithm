#include <bits/stdc++.h>

int findMajorityElement(int arr[], int n) {
	int fre=1;
	int i =1;
	int maj= arr[0];
while(i<n){
if (arr[i]==maj){
	fre++;
}

else{
	fre--;
	
	if(fre==0 && i<n-1){
	maj =arr[i+1];}
	
}
i++;


}
int count=0;
for( int i =0 ; i<n; i++){
	if(arr[i]==maj){
		count++;
	}
}
if( count>n/2){
	return maj;
}
else{
	return -1;
}

}
