// Counting the frequency of the number greater than 10^6 like 10^9..


class Solution {
public:
    vector<vector<int>> countFrequencies(vector<int>& nums) {
// vector
map<int, int>  mpp;
for(int i=0;i<nums.size(); i++){
    mpp[nums[i]]++;
}
vector<vector<int>> result;
        for(auto& pair : mpp) {
            result.push_back({pair.first, pair.second});
        }
        return result;
    }
};
