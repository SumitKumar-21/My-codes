 vector<int> sortArray(vector<int>& nums) {
        int n = nums.size();
        for(int i=0;i <n; i++){
            int x= nums[i];
            for(int j =0; j<n ; j++){
                if(nums[i]<nums[j]){
                    swap(nums[i],nums[j]);
                }
            }
        }
        return nums;
    }
